{
    "name": "angular-router-ex",
    "version": "2.0.1-alpha2",
    "description": "An Angular 2+ Router, custom route urls, route caching",
    "keywords": [
        "angular2",
        "typescript"
    ],
    "typings": "./router-ex.d.ts",
    "main": "./lib/index.js",
    "license": "MIT",
    "scripts": {
        "postinstall": "npm run webdriver:update",
        "ci:travis": "npm run test && npm run ngcompile",
        "clean:dll": "npm run rimraf -- dll",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "githubpages": "git subtree push --prefix dist origin gh-pages",
        "preclean:install": "npm run clean",
        "rimraf": "rimraf",
        "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base sample/",
        "start": "npm run server:dev",
        "webdriver-manager": "webdriver-manager",
        "webdriver:start": "npm run webdriver-manager start",
        "webdriver:update": "webdriver-manager update",
        "webpack-dev-server": "webpack-dev-server",
        "webpack": "webpack",
        "clean:ngc": "rimraf lib",
        "ngcompile": "npm run clean:ngc && ngc -p ./tsconfig.aot.json",
        "test:unit": "karma start",
        "protractor": "protractor ./e2e/protractor.conf.js",
        "build:test": "tsc -p e2e",
        "build:demo": "rimraf dist/* && webpack --config webpack.config.js --progress --profile",
        "test:run": "npm-run-all -p -r server:ci protractor",
        "test": "npm run test:unit && npm run build:test && npm run build:demo && npm run test:run",
        "test:e2e": "npm run build:test && npm run protractor",
        "server:ci": " pushstate-server dist 3000",
        "package": "ng-packagr -p ./src/ng-package.json",
        "publish": "npm publish ./src/dist"
    },
    "peerDependencies": {
        "rxjs": "^5.0.1",
